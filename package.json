    "migrate-db-test": "sequelize db:migrate --env test",
    "seed-db-test": "npm run seed-role && npm run seed-admin && npm run seed-type",
    "seed-role": "sequelize db:seed --seed 20170223122049-roles.js --env test",
    "seed-admin": "sequelize db:seed --seed 20170223122057-admin.js --env test",
    "seed-type": "sequelize db:seed --seed 20170223122105-types.js --env test"
  },
