{
  "name": "youdoc",
  "version": "1.0.0",
  "description": "Open, light-weight, document management system.",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm install rimraf -g",
    "babel-serve": "nodemon --exec babel-node ./server/server.js",
    "serve-app": "node dist/server.js",
    "start": "npm run build && npm run serve-app",
    "build": "rimraf dist/ && babel --copy-files ./server --out-dir dist/",
    "test--": "npm run migrate-db-test && npm run seed-db-test && npm run cover",
    "test": "NODE_ENV=test node_modules/babel-cli/bin/babel-node.js node_modules/babel-istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- server/tests/api/*.js",
    "setup-db-test": "npm run drop-db-test && npm run create-db-test",
    "create-db-test": "psql -c 'create database youdoc_test;' -U postgres",
    "drop-db-test": "psql -c 'drop database if exists youdoc_test;' -U postgres",
    "migrate": "sequelize db:migrate",
    "seed-db-test": "sequelize db:seed:all --env test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-fabolaji/youdoc.git"
  },
  "keywords": [
    "youdoc",
    "api",
    "rest",
    "document",
    "open-source"
  ],
  "author": "Abolaji Femi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-fabolaji/youdoc/issues"
  },
  "homepage": "https://github.com/andela-fabolaji/youdoc#readme",
  "devDependencies": {
    "codeclimate-test-reporter": "^0.4.1",
    "faker": "^4.1.0",
    "mocha": "^3.2.0"
  },
  "dependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-istanbul": "^0.12.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-register": "^6.22.0",
    "babel-watch": "^2.0.5",
    "bcrypt-node": "^0.1.0",
    "body-parser": "^1.16.0",
    "chai": "^3.5.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.10.0",
    "express": "^4.14.0",
    "faker": "^4.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-nodemon": "^2.2.1",
    "gulp-sass": "^3.1.0",
    "jsonwebtoken": "^7.2.1",
    "morgan": "^1.7.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.2",
    "sequelize-cli": "^2.5.1",
    "superagent": "^3.4.1",
    "supertest": "^3.0.0",
    "webpack-stream": "^3.2.0"
  }
}
